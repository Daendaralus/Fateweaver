{
  "id": "fateweaver-module",
  "title": "Fateweaver",
  "description": "",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 10,
    "verified": 10,
    "maximum": 11
  },
  "authors": [
    {
      "name": "Daendaralus",
      "url": "https://github.com/Daendaralus/"
    }
  ],
  "relationships": {
    "requires": [
			{
				"id": "orcnog-card-viewer",
				"type": "module",
				"manifest": "https://github.com/orcnog/orcnog-card-viewer/releases/latest/download/module.json"
			}
		]
  },
  "macros": [
    {
      "name": "Change one's Fate",
      "type": "script",
      "img": "icons/macro-icon.svg", // Optional: Provide an image for the macro
      "command": "./macros/macro.js", // The path to your macro's script
      "flags": {}
    }
  ],
  "esmodules": [
    "scripts/module.js"
  ],
  "scripts": [
    "scripts/lib/lib.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "socket": true,
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": "https://avatars2.githubusercontent.com/u/71292812?s=400&u=ccdb4eeb7abf551ca8f314e5a9bfd0479a4d3d41&v=4"
    }
  ]
}
